{
  "name": "aurelia-templating-resources",
  "version": "1.14.0",
  "description": "A standard set of behaviors, converters and other resources for use with the Aurelia templating library.",
  "keywords": [
    "aurelia",
    "templating",
    "html"
  ],
  "homepage": "http://aurelia.io",
  "bugs": {
    "url": "https://github.com/aurelia/templating-resources/issues"
  },
  "license": "MIT",
  "author": "Rob Eisenberg <rob@bluespire.com> (http://robeisenberg.com/)",
  "main": "dist/commonjs/aurelia-templating-resources.js",
  "module": "dist/native-modules/aurelia-templating-resources.js",
  "typings": "dist/types/aurelia-templating-resources.d.ts",
  "repository": {
    "type": "git",
    "url": "http://github.com/aurelia/templating-resources"
  },
  "scripts": {
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "test:debugger": "karma start --browsers ChromeDebugging",
    "lint": "eslint .",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "build:dts": "dts-bundle-generator -o dist/types/aurelia-templating-resources.d.ts src/aurelia-templating-resources.ts",
    "postbuild": "npm run build:dts",
    "typedoc": "typedoc src/aurelia-templating-resources.ts --json doc/api.json",
    "posttypedoc": "node doc/cleanup.js",
    "changelog": "conventional-changelog -p angular -i doc/CHANGELOG.md -s",
    "precut-release": "npm run test && npm run lint",
    "cut-release": "npm run build && npm run typedoc && npm version --git-tag-version false",
    "postcut-release": "node doc/bower-version.js && npm run changelog"
  },
  "files": [
    "dist",
    "src",
    "doc/api.json",
    "doc/CHANGELOG.md",
    "typings.json",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "aurelia-binding": "^2.0.0",
    "aurelia-dependency-injection": "^1.0.0",
    "aurelia-loader": "^1.0.0",
    "aurelia-logging": "^1.0.0",
    "aurelia-metadata": "^1.0.0",
    "aurelia-pal": "^1.3.0",
    "aurelia-path": "^1.1.7",
    "aurelia-task-queue": "^1.0.0",
    "aurelia-templating": "^1.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/estree": "0.0.51",
    "@types/jasmine": "^3.10.4",
    "aurelia-bootstrapper": "^2.3.3",
    "aurelia-testing": "^1.0.0",
    "aurelia-webpack-plugin": "^5.0.3",
    "conventional-changelog": "^3.1.3",
    "conventional-changelog-cli": "^2.2.2",
    "dts-bundle-generator": "^6.5.0",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-conventional-changelog": "^2.0.35",
    "html-loader": "^0.5.5",
    "jasmine-core": "^3.2.1",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^1.1.2",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^4.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "rimraf": "^2.7.1",
    "rollup": "^2.70.1",
    "ts-loader": "^9.2.8",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "yargs": "^4.8.1"
  },
  "volta": {
    "node": "14.19.1",
    "npm": "6.14.16"
  }
}
